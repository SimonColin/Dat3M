X86 m44iif  (MFourFourInternalInternalFence)
"plain iriw, no barriers, internal rfs, fences"
{
 x=0; y=0;
}

P0           | P1          ;
MOV [x],1    | MOV [y],2   ;
MFENCE       | MFENCE      ;
MOV EAX,[x]  | MOV EBX,[y] ;
MOV EBX,[y]  | MOV EAX,[x] ;

exists(0:EAX=1 /\ 0:EBX=0 /\ 1:EAX=0 /\ 1:EBX=2)
