X86 amd3bis  (amdThreeBis)
{
 x = 0; y = 0;
}
  P0            |  P1           ;
  mov [x], 1  	|  mov [y], 1 	;
  mov ECX,[z]   |  mov ECX,[z]  ;
  mov ECX,[t]   |  mov ECX,[t]  ;
  mov [x], 2  	|  mov [y], 2 	;
  mov EAX, [y] 	|  mov EBX, [x] ;

 exists (0:EAX = 1 /\ 1:EBX = 1);

(* This alternative to amd3 yields a better
   repartition of outcomes, at least on conti *)