X86 iwp2.3.a/amd4.lfence (iwpTwoThreeaLFence) "Loads may be reordered with older stores to different locations"
{x = 0; y = 0};

        P0      |       P1      ;
  mov [x], 1  	|  mov [y], 1 	;
  lfence        |  lfence       ;
  mov EAX, [y] 	|  mov EBX, [x] ;

exists (P0:EAX = 0 /\ P1:EBX = 0);

<<
show flags false
show 3
genprog generated/iwp2.3.a-amd4.lfence-prog.tex
essdump generated/iwp2.3.a-amd4.lfence-ess.dot
>>
<<
show flags false
show 1
readfrom generated/iwp2.3.a-amd4.lfence-rf.dot
>>

