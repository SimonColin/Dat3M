X86 rwc-fenced (rwcfenced) "Read-to-Write Causality, from Boehm and Adve, Fig. 6"
{
 x=0;y=0;
}
  P0           |  P1             | P2           ;
  mov [x],1    |  mov EAX, [x]   | mov [y],1    ;
               |  mov EBX, [y]   | mfence       ;
               |                 | mov ECX,[x]  ;

exists (1:EAX = 1 /\ 1:EBX=0 /\ 2:ECX=0);