X86 amd6 (amdSix)
{
 x = 0; y = 0; 2:EAX = 0; 2:EBX = 0; 3:ECX = 0; 3:EDX = 0;
}

   P0             |  P1             |  P2            |  P3          ;
   mov [x], 1     |  mov [y],  1    |  mov EAX, [x]  |  mov ECX, [y];
                  |                 |  mov EBX, [y]  |  mov EDX, [x];

exists (2:EAX = 1 /\ 2:EBX = 0 /\ 3:ECX = 1 /\ 3:EDX = 0);

(* we don't expect to see that final state on actual implementations *)