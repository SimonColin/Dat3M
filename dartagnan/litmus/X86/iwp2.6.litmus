X86 iwp2.6 (iwpTwoSix) "Total order on stores to the same location"
{
 x = 0;
}
  P0            |  P1             |  P2	            |  P3           ;
  mov [x], 1  	|  mov [x], 2     |  mov EAX, [x]	|  mov ECX, [x]	;
               	|      	    	  |  mov EBX, [x]	|  mov EDX, [x] ;
exists (2:EAX = 1 /\ 2:EBX = 2 /\ 3:ECX = 2 /\ 3:EDX = 1);