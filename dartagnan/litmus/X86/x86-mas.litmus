X86 x86-mas  (X86MTenFence)
" ST ; LD || LD ; ST hexa version, with mfences"
{
}

P0          |P1          |P2          |P3        ;
mov [x],1   |mov EAX,[y] |            |          ;
mfence      |mfence      |            |          ;
mov EAX,[y] |mov [x],2   |mov EAX,[x] |mov [y],2 ;
locations [0:EAX ; 1:EAX ; 2:EAX ; x ;]

