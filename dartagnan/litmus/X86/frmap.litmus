 x = 0; y = 0;
}
  P0            |  P1             |  P2	           |  P3            ;
  mov [x], 1  	|  mov [y], 1     |  mov [x], 2	   |  mov ECX, [x]	;
               	|  mov EAX, [y]   |   	           |  mov EDX, [y]  ;
               	|  mov EBX, [x]   | 		  	   |  	    	    ;

exists (1:EAX = 1 /\ 1:EBX = 1 /\ 3:ECX = 2 /\ 3:EDX = 0)