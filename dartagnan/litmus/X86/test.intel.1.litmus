X86 iwp2.1/amd1 (iwp) "litmus test"
(* Test iwp2.1/amd1 :  Loads are not reordered with other loads and stores are not reordered with other stores *)
{
 x=0; y=0;
}

  P0          |  P1           ;
  mov [x], 1  |  mov EAX, [y] ;
  mov [y], 1  |  mov EBX, [x] ;
              |               ;
exists (1:EAX = 1 /\ 1:EBX = 0)